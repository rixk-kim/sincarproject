<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageButton
        android:id="@+id/imBtnBack_rental_list_filter_price"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:background="@color/base_background_color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/prev_image_selector" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="132dp"
        android:layout_height="20dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="72dp"
        android:text="가격 설정"
        android:fontFamily="@font/nanumsquarebold"
        android:textColor="@color/agent_color_1NoOpp"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="108dp"
        android:fontFamily="@font/nanumsquareregular"
        android:textColor="@color/agent_color_1_80Opp"
        android:text="1일 요금의 범위를 선택해주세요."
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <RadioGroup
        android:id="@+id/rg_rental_list_filter_price"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="154dp"
        app:layout_constraintVertical_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rg_rental_list_filter_price2"
        app:layout_constraintTop_toTopOf="parent">

        <!-- TODO 가격 설정 아이템1 -->
        <RadioButton
            android:id="@+id/rbPriall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12.5dp"
            android:checked="true"
            android:text="  전체"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />

        <!-- TODO 가격 설정 아이템2 -->
        <RadioButton
            android:id="@+id/rbPri20u"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  20만원 이하"
            android:layout_marginVertical="12.5dp"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />

        <!-- TODO 가격 설정 아이템3 -->
        <RadioButton
            android:id="@+id/rbPri40u"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  40만원 이하"
            android:layout_marginVertical="12.5dp"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />

        <!-- TODO 가격 설정 아이템4 -->
        <RadioButton
            android:id="@+id/rbPri50o"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  50만원 이상"
            android:layout_marginVertical="12.5dp"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />


    </RadioGroup>

    <RadioGroup
        android:id="@+id/rg_rental_list_filter_price2"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:layout_marginEnd="28dp"
        android:layout_marginTop="154dp"
        app:layout_constraintVertical_weight="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rg_rental_list_filter_price"
        app:layout_constraintTop_toTopOf="parent">

        <!-- TODO 가격 설정 아이템5 -->
        <RadioButton
            android:id="@+id/rbPri10u"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  10만원 이하"
            android:layout_marginBottom="12.5dp"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />

        <!-- TODO 가격 설정 아이템6 -->
        <RadioButton
            android:id="@+id/rbPri30u"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  30만원 이하"
            android:layout_marginVertical="12.5dp"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />

        <!-- TODO 가격 설정 아이템7 -->
        <RadioButton
            android:id="@+id/rbPri50u"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  50만원 이하"
            android:layout_marginVertical="12.5dp"
            android:textColor="@color/agent_color_1NoOpp"
            android:button="@drawable/radio_button_selector"
            android:fontFamily="@font/nanumsquareregular"
            android:textSize="15sp" />
    </RadioGroup>

    <!-- TODO 최종 적용 버튼 -->
    <Button
        android:id="@+id/btnPriceCheck"
        android:layout_width="304dp"
        android:layout_height="52dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/bg_custom_button2"
        android:fontFamily="@font/nanumsquareregular"
        android:text="적용"
        android:textColor="@color/base_background_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>